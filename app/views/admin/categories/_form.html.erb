<%= form_with(model: [ :admin, category], local: true) do |f| %>

  <%= render 'message' %>
  
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <br/>
  <div>
    <%= f.submit %>
  </div>
<% end %>